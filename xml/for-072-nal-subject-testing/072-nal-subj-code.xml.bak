      <datafield tag="245" ind1="1" ind2="0">
         <subfield code="a">Evaluating height structure in Scots pine forests using marked point
                processes</subfield>
      </datafield>
      <datafield tag="300" ind1=" " ind2=" ">
         <subfield code="a">p. 1924-1934.</subfield>
      </datafield>
      <datafield tag="500" ind1=" " ind2=" ">
         <subfield code="a">Includes references</subfield>
