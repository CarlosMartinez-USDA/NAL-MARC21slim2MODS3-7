        <datafield tag="245" ind1="1" ind2="0">
            <subfield code="a">Ammonium Bicarbonate-DTPA and DTPA Extractions of Sludge-amended Soils</subfield>
        </datafield>
        <datafield tag="300" ind1=" " ind2=" ">
            <subfield code="a">p. 125-130.</subfield>
