      <titleInfo>
         <title>Biosynthesis of human myeloperoxidase</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">William M.</namePart>
         <namePart type="family">Nauseef</namePart>
         <displayForm>Nauseef, William M.</displayForm>
      </name>
