      <titleInfo>
         <title>Parasitized Aphids Are Inferior Prey for a Coccinellid Predator: Implications for Intraguild Predation</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">Einat</namePart>
         <namePart type="family">Bilu</namePart>
