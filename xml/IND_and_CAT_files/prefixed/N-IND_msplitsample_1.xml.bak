      <titleInfo>
         <title>Ammonium Bicarbonate-DTPA and DTPA Extractions of Sludge-amended Soils</title>
      </titleInfo>
      <name type="personal" usage="primary">
         <namePart type="given">K. A.</namePart>
         <namePart type="family">Barbarick</namePart>
         <displayForm>Barbarick, K. A.</displayForm>
      </name>
